
import java.io.*;
import java.util.*;

public class FileInput{

   public static void main(String args[]) throws IOException{
        StringTokenizer st;
        String[] airport = new String[500];
        String[][] route= new String[700][700];
        int i =0;
        int k=0;

           BufferedReader br = new BufferedReader(
                     new FileReader("input.txt"));

           String line = br.readLine();
           //System.out.println(line);
           //;double avg = 0.0;
           //int num = 0, count = 0;

           while(line != null){
        	   //System.out.println(line);
        	   if(!line.contains("#*")){
        		   
        		   if(line.length()==3 && !line.matches("[0-9]+")){
        			airport[i]=line; 
        			//System.out.println(airport[i]);
        			i++;
        			   
        		   }
        	   		if(line.contains("R*,")){
        		   	
                   st = new StringTokenizer(line,",");

                   
                   for(int j = 0; i < st.countTokens(); j++){
                	  route[k][j]=st.nextToken();
                        
                   }
                   k++;
        	   		}
        	   }
                   line = br.readLine();
           }

           br.close();
/*
           avg = avg/count;
           System.out.println("Avg: " + avg);
           
*/
          // System.out.println(airport.length);
           for(int z=0;z<airport.length;z++){
        	   
        	   System.out.println(airport[z]);
           }
           
        
        
   } //main
} //class