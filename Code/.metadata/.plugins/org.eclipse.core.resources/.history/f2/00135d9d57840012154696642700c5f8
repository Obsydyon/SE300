
import java.io.*;
import java.util.*;

public class FileInput{

   public static void main(String args[]) throws IOException{
        StringTokenizer st;
        String[] airport = new String[10];
        String[][] route= new String[16][7];
        int i =0;
        int k=0;

           BufferedReader br = new BufferedReader(
                     new FileReader("input.txt"));

           String line = br.readLine();
           //System.out.println(line);
           //;double avg = 0.0;
           //int num = 0, count = 0;

           while(line != null){
        	   if(!line.contains("#")){
        		   //System.out.println(line);
        		   if(line.length()==3 && !line.matches("[0-9]+")){
        			airport[i]=line; 
        			i++;
        			   
        		   }
        	   		if(line.contains(",")){
                   st = new StringTokenizer(line,", ");
                   System.out.println("Number of tokens: "+st.countTokens());

                   
                   for(int j = 0; j<7; j++){
                	  route[k][j]=st.nextToken();
                	  System.out.println(k+" "+j);
                	  //System.out.println(route[k][j]);
                        
                   }
                   k++;
        	   		}
        	   }
                   line = br.readLine();
           }

           br.close();
/*
           avg = avg/count;
           System.out.println("Avg: " + avg);
           
*/
           
          // System.out.println(airport.length);
           for(int z=0;z<airport.length;z++){
        	   
        	   System.out.println(airport[z]);
           }
           
           for(int z=0;z<route.length;z++){
        	   
        	   for(int h=0;h<route[z].length;h++){
        		   
        		   System.out.println(route[z][h]);
        		   
        	   }
        	   
           }
           
        
        
   } //main
} //class